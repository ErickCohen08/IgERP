	<parameter name="tipo_comprobante" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
g.id_guia,
rtrim(d.serie) as 'Serie',
rtrim(g.numero_guia) as 'NumeroGuia',

rtrim(g.punto_partida) as 'P_partida',
rtrim(g.punto_llegada) as 'P_llegada',

convert(varchar, g.fecha, 103) as 'Fecha',
rtrim(c.razon_social) as 'razon_social_cliente',
rtrim(c.ruc) as 'ruc_cliente',

rtrim(v.marca) as 'Vehiculo_marca',
rtrim(v.placa) as 'Vehiculo_placa',
rtrim(v.nro_inscripcion) as 'Vehiculo_nro_inscripcion',
rtrim(con.nro_licencia) as 'Conductor_licencia',

rtrim(emp.razon_social) as 'EmpTransporte_razonsocial',
rtrim(emp.ruc) as 'EmpTransporte_ruc',

rtrim(g.motivo_traslado) as 'Mot_Traslado',

rtrim(dg.descripcion) as 'Descripcion_guia',
dg.cantidad as 'Cantidad_guia',
rtrim(dg.unidad) as 'Unidad_guia',
dg.peso as 'Peso_guia'



from tguia g, tdocumentos d, tcliente c, tvehiculo v, tconductor con, tempresatransporte emp, tdetalle_guia dg
where
g.id_cliente=c.id_cliente and
g.id_documento=d.id_documento and
g.id_vehiculo=v.id_vehiculo and
g.id_conductor=con.id_conductor and
g.id_empresatransporte=emp.id_empresatransporte and
g.id_guia=dg.id_guia and
g.id_guia=$P{id_guia}]]>
	</queryString>
