	<queryString>
		<![CDATA[select
rtrim(f.numero_factura) as 'Numero de Factura',
rtrim(d.serie) as 'Serie',

rtrim(c.razon_social) as 'Razon Social',
rtrim(c.direccion) as 'Direccion',
rtrim(c.ruc) as 'R.U.C.',

rtrim(f.ordencompra) as 'Ordendecompra',
rtrim(tp.descripcion) as 'TipoPago',
convert(varchar, f.vencimiento, 103) as 'Vencimiento',
convert(varchar, f.fecha, 103) as 'Fecha',
rtrim(f.moneda) as 'Moneda',

rtrim(df.descripcion) as 'Descripcion',
rtrim(df.unidad) as 'Uunid.',
df.cantidad as 'Cant.',
df.precio_unitario as 'PreUnit',
df.descuento_por as 'DescuentoPor',
df.precio_total as 'PreTotal',

f.total_letras as 'TotalLetras',
f.descuento as 'Descuento',
f.sub_total as 'SubTotal',
f.calculo_igv as 'CalculoIGV',
i.igv as 'I.G.V.',
f.total as 'TOTAL',
f.simbolo_moneda as 'SomboloMoneda',
rtrim(f.observaciones) as 'Observaciones'



from tfactura f, tcliente c, tigv i, tdocumentos d, tusuario u , tdetalle_factura df, ttipopago tp
where
f.id_cliente=c.id_cliente and
f.id_igv=i.id_igv and
f.id_documento=d.id_documento and
f.id_usuario=u.id_usuario and
df.id_factura=f.id_factura and
f.id_tipopago = tp.id_tipopago and
f.id_factura=$P{id_factura}]]>
	</queryString>
